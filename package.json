{
  "scripts": {
    "dev": "parcel src/index.html",
    "cp:image": "cp -r ./src/img ./docs/",
    "clear": "rm -rf ./docs",
    "parcel:build": "parcel build src/index.html --public-url=. --out-dir=docs",
    "build": "npm run clear && npm run parcel:build && npm run cp:image"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.4",
    "@noliaki/eslint-config-prettier": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "face-api.js": "^0.20.1"
  }
}
